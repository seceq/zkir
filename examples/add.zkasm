; Simple addition program
; Input: two numbers a and b
; Output: a + b

.section .text
.global _start

_start:
    read    a0              ; a0 = first input
    read    a1              ; a1 = second input
    add     rv, a0, a1      ; rv = a0 + a1
    write   rv              ; output result
    halt

.section .data
; No data needed
