; Simple addition program
; Input: two numbers a and b
; Output: a + b

.entry main

main:
    READ r1         ; r1 = a (first input)
    READ r2         ; r2 = b (second input)
    ADD r3, r1, r2  ; r3 = a + b
    WRITE r3        ; output result
    COMMIT r3       ; commit as public output
    HALT
